public class Print10RowsOf10
{
  public static void main(String[] args)
  {
    //    for (int i = 1; i < 101; i++)
    //    {
    //      System.out.print(i + " ");
    //      if (i != 1 && i % 10 == 0)
    //      {
    //        System.out.println("");
    //      }
    //    }
    for (int i = 0; i < 10; i++)
    {
      for (int j = 1; j < 10; j++)
      {
        System.out.print((j * i) + " ");
      }
      System.out.println();
    }
  }
}
