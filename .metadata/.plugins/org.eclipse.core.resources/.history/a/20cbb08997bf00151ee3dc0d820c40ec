import java.util.Scanner;

public class printNIntegerson10rows
{
  public static void main(String[] args)
  {
    Scanner scan = new Scanner(System.in);
    int N = scan.nextInt();
    System.out.print("How many integers do you want to print?");
    System.out.println();
    for (int i = 0; i < 10; i++)
    {
      for (int j = 0; j < N; j++)
      {
        System.out.print(N + " ");
      }
      System.out.println();
    }
  }
}
